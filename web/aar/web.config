<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="Default.htm" />
                <add value="Default.asp" />
                <add value="index.htm" />
                <add value="index.html" />
                <add value="iisstart.htm" />
                <add value="index.php" />
                <add value="default.aspx" />
            </files>
        </defaultDocument>
        <rewrite>
            <rules>
              <rule name="rule 1x" stopProcessing="true">
              	<match url="^replay/([\w]+)/?$"  ignoreCase="true" />
              	<action type="Rewrite" url="/replay.php?id={R:1}        # Replay URLs"  />
              </rule>
              <rule name="rule 2x" stopProcessing="true">
              	<match url="^replay/([\w]+)/frame/([0-9]+)/?$"  ignoreCase="true" />
              	<action type="Rewrite" url="/replay.php?id={R:1}&amp;frame={R:2}        # Replay URLs"  />
              </rule>
              <rule name="rule 3x" stopProcessing="true">
              	<match url="^live/?$"  ignoreCase="true" />
              	<action type="Rewrite" url="/live.php        # Live URL"  />
              </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
